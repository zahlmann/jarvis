[Unit]
Description=jarvis-phi telegram service
After=network.target

[Service]
Type=simple
User=YOUR_USER
WorkingDirectory=/home/YOUR_USER/code/jarvis-phi
EnvironmentFile=/home/YOUR_USER/code/jarvis-phi/.env
ExecStart=/usr/bin/env bash -lc 'GOCACHE=/tmp/go-build-cache GOMODCACHE=/tmp/go-mod-cache GOPATH=/tmp/go go run ./cmd/server'
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
