# Required
TELEGRAM_BOT_TOKEN=
TELEGRAM_WEBHOOK_SECRET=

# phi auth
PHI_AUTH_MODE=chatgpt
PHI_CHATGPT_ACCESS_TOKEN=
# Optional advanced override; usually inferred automatically.
PHI_CHATGPT_ACCOUNT_ID=
# Required for memory embeddings and voice transcription.
OPENAI_API_KEY=

# Runtime
JARVIS_PHI_LISTEN_ADDR=:8080
JARVIS_PHI_TZ=Europe/Vienna
JARVIS_PHI_DATA_DIR=./data
JARVIS_PHI_TOOL_ROOT=../
JARVIS_PHI_MODEL=gpt-5.3-codex
# none|minimal|low|medium|high|xhigh
JARVIS_PHI_THINKING=xhigh
JARVIS_PHI_MEMORY_EMBEDDING_MODEL=text-embedding-3-small
# Preferred name Jarvis should use when addressing you (set by wake-jarvis.sh)
JARVIS_USER_NAME=
JARVIS_PHI_DEFAULT_CHAT_ID=

# Heartbeat (internal scheduler)
JARVIS_PHI_HEARTBEAT_ENABLED=true

# Voice out
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=EkK5I93UQWFDigLMpZcX
JARVIS_PHI_TRANSCRIPTION_ENABLED=true
JARVIS_PHI_VOICE_REPLY_ENABLED=false

# Bring integration (native Go client)
BRING_EMAIL=
BRING_PASSWORD=
# Optional: select specific list by UUID or name, otherwise first list is used
BRING_LIST_UUID=
BRING_LIST_NAME=
# Optional overrides
BRING_COUNTRY=US
BRING_ITEM_CONTEXT=en-US
BRING_API_BASE_URL=https://api.getbring.com/rest

# Optional overrides
JARVIS_PHI_TELEGRAM_API_BASE=https://api.telegram.org
